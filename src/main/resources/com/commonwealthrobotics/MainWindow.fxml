                              <ImageView fx:id="modeling" fitHeight="30.0" fitWidth="30.0" onDragDetected="#onModeling" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@SourceIcon.png" />
                                 </image>
                              </ImageView>
